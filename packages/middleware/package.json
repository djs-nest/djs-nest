{
  "name": "@djs-nest/middleware",
  "version": "1.1.0",
  "description": "Djs Nest Middleware Module to receive Discord directly via http request",
  "author": "iam10k",
  "license": "Apache-2.0",
  "keywords": [
    "nestjs",
    "discord",
    "middleware",
    "interaction",
    "discord bot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djs-nest/djs-nest.git"
  },
  "bugs": {
    "url": "https://github.com/djs-nest/djs-nest/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/middleware/*'",
    "release": "cliff-jumper"
  },
  "dependencies": {
    "tweetnacl": "^1.0.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^1.10.0",
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "latest",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-jest": "28.0.8",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".*/index.ts$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 75,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
